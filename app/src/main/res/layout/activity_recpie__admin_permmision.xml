<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Recpie_Admin_permmision">


    <ImageView
        android:id="@+id/Menu_icon_btn"
        android:layout_width="49dp"
        android:layout_height="47dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="71dp"
        app:layout_constraintEnd_toStartOf="@+id/Logo_text"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu" />

    <ImageView
        android:id="@+id/User_icon_btn"
        android:layout_width="49dp"
        android:layout_height="48dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="42dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/Logo_text"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_person" />

    <TextView
        android:id="@+id/Logo_text"
        android:layout_width="111dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:text="Fooding"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/User_icon_btn"
        app:layout_constraintStart_toEndOf="@+id/Menu_icon_btn"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Hello_user_txt"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="95dp"
        android:text="Hello user"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/User_icon_btn"
        app:layout_constraintStart_toEndOf="@+id/Menu_icon_btn"
        app:layout_constraintTop_toBottomOf="@+id/Logo_text" />

    <ImageView
        android:id="@+id/SCrool_icon2"
        android:layout_width="344dp"
        android:layout_height="15dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_description"
        app:srcCompat="@drawable/ic_longcircle2" />

    <ImageView
        android:id="@+id/SCrool_icon"
        android:layout_width="344dp"
        android:layout_height="15dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="42dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Hello_user_txt"
        app:srcCompat="@drawable/ic_longcircle2" />

    <TextView
        android:id="@+id/image_description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="79dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="21dp"
        android:text="Admins Permmision Area"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/SCrool_icon2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SCrool_icon" />

    <TextView
        android:id="@+id/image_description3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="host"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        tools:layout_editor_absoluteY="255dp" />

    <TextView
        android:id="@+id/image_description4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:text="date"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/image_description3" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="84dp"
        android:layout_height="89dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toStartOf="@+id/image_description3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SCrool_icon2"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/approve_button"
        android:layout_width="118dp"
        android:layout_height="29dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#4CAF50"
        android:text="Approve"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/delete_recipe_buttn"
        android:layout_width="117dp"
        android:layout_height="28dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#E60D0D"
        android:text="Delete "
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/approve_button"
        app:layout_constraintHorizontal_bias="0.312"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/date_text"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="278dp" />

    <TextView
        android:id="@+id/host_name_text"
        android:layout_width="180dp"
        android:layout_height="20dp"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="258dp" />

    <EditText
        android:id="@+id/edit_text_inng"
        android:layout_width="365dp"
        android:layout_height="64dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="343dp" />

    <EditText
        android:id="@+id/edit_text_howTo"
        android:layout_width="362dp"
        android:layout_height="186dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="459dp" />

    <EditText
        android:id="@+id/edit_text_recipeName"
        android:layout_width="213dp"
        android:layout_height="34dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/image_description3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SCrool_icon2"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>